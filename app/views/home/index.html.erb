<% if user_signed_in? %>
<h1>My Projects</h1>

<%= link_to 'New Project', new_project_path %>
<br>
<br>
    <% @projects.each do |project| %>
        <div class="item">
            <div class="name">
                <h2><%= link_to project.name, project %></h2>
            </div>
            <div class="description"><%= project.description %></div>
        </div>
    <% end %>
<% end %>